    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>f6b2e9a5-2840-49cb-9bb3-e95d2eff8aa2</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.2.2" />
